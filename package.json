{
  "name": "booking-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint:ox": "oxlint src",
    "knip": "knip",
    "knip:production": "knip --production",
    "preview": "vite preview",
    "prepare": "husky",
    "knip:check": "knip --no-exit-code || exit 0",
    "generate:api": "openapi-typescript http://localhost:1337/api-json -o src/lib/api-schema.d.ts",
    "generate:api:local": "openapi-typescript src/lib/api-schema.json -o src/lib/api-schema.d.ts"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "oxlint",
      "eslint --fix"
    ],
    "src/**/*.{ts,tsx}": "pnpm knip:check"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-form": "^1.23.8",
    "@tanstack/react-query": "^5.90.5",
    "@tanstack/react-router": "^1.133.22",
    "axios": "^1.12.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "i18next": "^25.6.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.546.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-i18next": "^16.1.5",
    "tailwind-merge": "^3.3.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/postcss": "^4.1.15",
    "@tanstack/router-devtools": "^1.133.22",
    "@tanstack/router-plugin": "^1.133.22",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "knip": "^5.66.2",
    "lint-staged": "^16.2.6",
    "openapi-fetch": "^0.15.0",
    "openapi-typescript": "^7.10.1",
    "oxlint": "^1.24.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.15",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7"
  }
}
